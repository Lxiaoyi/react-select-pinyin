{
  "name": "react-select-pinyin",
  "version": "0.7.0",
  "description": "React 选择控件（支持拼音搜索）",
  "main": "lib/SelectPinyin.js",
  "scripts": {
    "start": "node worker.js",
    "prepublish": "cp ./node_modules/react-select/dist/default.css ./lib/default.css && ./node_modules/.bin/jsx -x jsx --harmony --no-cache-dir ./src/ ./lib/",
    "test": "jest",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xcatliu/react-select-pinyin.git"
  },
  "keywords": [
    "react",
    "react-component",
    "select",
    "pinyin",
    "select-pinyin",
    "react-select",
    "react-pinyin",
    "react-select-pinyin",
    "multiselect",
    "combobox",
    "input",
    "form",
    "ui"
  ],
  "author": "xcatliu <xcatliu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xcatliu/react-select-pinyin/issues"
  },
  "homepage": "https://github.com/xcatliu/react-select-pinyin",
  "dependencies": {
    "pinyin": "^2.4.4",
    "react": "^0.13.3",
    "react-select": "^0.4.9",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^5.5.5",
    "babel-loader": "^5.1.4",
    "brfs": "^1.4.0",
    "ecstatic": "^0.8.0",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.5.0",
    "insert-css": "^0.2.0",
    "jest": "^0.1.40",
    "jest-cli": "^0.4.12",
    "react-tools": "^0.13.3",
    "transform-loader": "^0.2.2",
    "webpack": "^1.9.10"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-select",
      "<rootDir>/node_modules/pinyin",
      "<rootDir>/node_modules/xtend"
    ]
  }
}
